{
  "name": "borsh-tester",
  "description": "Borsh Tester scripts",
  "main": "test-borsh.ts",
  "scripts": {
    "test-borsh": "tsx ./test-borsh.ts",
    "test-protobuf": "tsx ./test-protobuf.ts",
    "fix-lint": "tslint -c tslint.json '**/*.ts' --fix --exclude './protobuftypes.ts'",
    "gen-protobuf": "npx protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. --ts_proto_opt=outputServices=grpc-js --proto_path=./proto ./proto/protobuftypes.proto"
  },
  "author": "Fuel Labs",
  "license": "(MIT AND Apache-2.0)",
  "dependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "bignumber.js": "^9.1.2",
    "borsh": "git+https://github.com/near/borsh-js",
    "buffer": "^6.0.3",
    "dotenv": "^16.4.5",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "node-ts": "^6.1.3",
    "ts-node": "^10.7.0",
    "ts-proto": "^2.2.7",
    "tslib": "^2.8.1",
    "tslint": "^5.20.1",
    "typescript": "^5.6.3"
  }
}
