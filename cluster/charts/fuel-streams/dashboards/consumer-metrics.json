{
    "annotations": {
        "list": [
            {
                "builtIn": 1,
                "datasource": {
                    "type": "grafana",
                    "uid": "-- Grafana --"
                },
                "enable": true,
                "hide": true,
                "iconColor": "rgba(0, 211, 255, 1)",
                "name": "Annotations & Alerts",
                "type": "dashboard"
            },
            {
                "datasource": {
                    "type": "prometheus",
                    "uid": "__PROMETHEUS_DATA_SOURCE__"
                },
                "enable": true,
                "expr": "sum(changes(kube_deployment_status_observed_generation{deployment=\"consumer\"}[5m])) by (deployment)",
                "iconColor": "#5794F2",
                "name": "Deployments",
                "tagKeys": [
                    "deployment"
                ],
                "titleFormat": "Consumer Deployment"
            }
        ]
    },
    "links": [
        {
            "asDropdown": true,
            "icon": "external-link-alt",
            "includeVars": true,
            "keepTime": true,
            "tags": [
                "fuel-streams"
            ],
            "targetBlank": false,
            "title": "Related Dashboards",
            "type": "dashboards"
        }
    ],
    "panels": [
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 0
            },
            "title": "Resource Usage",
            "type": "row"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "CPU Cores",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 80
                            },
                            {
                                "color": "red",
                                "value": 90
                            }
                        ]
                    },
                    "unit": "cores"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 1
            },
            "targets": [
                {
                    "expr": "sum by (pod) (rate(container_cpu_usage_seconds_total{container=\"consumer\", pod=~\"$pod\"}[5m]))",
                    "legendFormat": "{{pod}}"
                }
            ],
            "title": "CPU Usage by Pod [5m]",
            "description": "CPU consumption in cores per pod",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Memory",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "unit": "bytes"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 1
            },
            "targets": [
                {
                    "expr": "avg_over_time(container_memory_working_set_bytes{container=\"consumer\", pod=~\"$pod\"}[5m])",
                    "legendFormat": "{{pod}} - usage"
                },
                {
                    "expr": "avg_over_time(kube_pod_container_resource_limits{container=\"consumer\", pod=~\"$pod\", resource=\"memory\"}[5m])",
                    "legendFormat": "{{pod}} - limit"
                }
            ],
            "title": "Memory Usage by Pod",
            "description": "Memory usage and limits per pod",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Network I/O",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "unit": "Bps"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 9
            },
            "targets": [
                {
                    "expr": "sum by (pod) (rate(container_network_receive_bytes_total{pod=~\"$pod\"}[5m]))",
                    "legendFormat": "{{pod}} - receive"
                },
                {
                    "expr": "sum by (pod) (rate(container_network_transmit_bytes_total{pod=~\"$pod\"}[5m]))",
                    "legendFormat": "{{pod}} - transmit"
                }
            ],
            "title": "Network I/O by Pod [5m]",
            "description": "Network traffic in/out per pod",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Percentage",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        },
                        "max": 100,
                        "min": 0
                    },
                    "unit": "percent"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 9
            },
            "targets": [
                {
                    "expr": "sum by (pod) (rate(container_cpu_usage_seconds_total{container=\"consumer\", pod=~\"$pod\"}[5m])) / sum by (pod) (kube_pod_container_resource_limits{container=\"consumer\", pod=~\"$pod\", resource=\"cpu\"}) * 100",
                    "legendFormat": "{{pod}} - CPU %"
                },
                {
                    "expr": "avg_over_time(container_memory_working_set_bytes{container=\"consumer\", pod=~\"$pod\"}[5m]) / avg_over_time(kube_pod_container_resource_limits{container=\"consumer\", pod=~\"$pod\", resource=\"memory\"}[5m]) * 100",
                    "legendFormat": "{{pod}} - Memory %"
                }
            ],
            "title": "Resource Usage Percentage",
            "description": "CPU and Memory usage as percentage of limits",
            "type": "timeseries"
        },
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 18
            },
            "title": "Processing Metrics",
            "type": "row"
        },
        {
            "targets": [
                {
                    "expr": "rate(consumer_metrics_duration_milliseconds_sum{subject=~\"$subject\"}[5m]) / rate(consumer_metrics_duration_milliseconds_count{subject=~\"$subject\"}[5m])",
                    "legendFormat": "{{subject}} - avg"
                },
                {
                    "expr": "rate(consumer_metrics_packets_sum{subject=~\"$subject\"}[5m]) / rate(consumer_metrics_packets_count{subject=~\"$subject\"}[5m])",
                    "legendFormat": "{{subject}} - packets"
                }
            ],
            "title": "Processing Duration and Packets [5m]",
            "description": "Average processing time and packet count per subject"
        },
        {
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum by (le, subject) (rate(consumer_metrics_duration_milliseconds_bucket{subject=~\"$subject\"}[5m])))",
                    "legendFormat": "p95 - {{subject}}"
                },
                {
                    "expr": "histogram_quantile(0.99, sum by (le, subject) (rate(consumer_metrics_duration_milliseconds_bucket{subject=~\"$subject\"}[5m])))",
                    "legendFormat": "p99 - {{subject}}"
                }
            ],
            "title": "Processing Duration Percentiles [5m]",
            "description": "95th and 99th percentiles of processing duration"
        },
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 27
            },
            "title": "Throughput and Errors",
            "type": "row"
        },
        {
            "targets": [
                {
                    "expr": "sum by (subject) (rate(consumer_metrics_messages_processed_total{subject=~\"$subject\"}[5m]))",
                    "legendFormat": "{{subject}}"
                }
            ],
            "title": "Message Processing Rate [5m]",
            "description": "Number of messages processed per second by subject"
        },
        {
            "targets": [
                {
                    "expr": "100 * (1 - (sum(rate(consumer_metrics_error_total{subject=~\"$subject\"}[5m])) / sum(rate(consumer_metrics_messages_processed_total{subject=~\"$subject\"}[5m]))))",
                    "legendFormat": "Success Rate"
                }
            ],
            "title": "Message Processing Success Rate [5m]",
            "description": "Percentage of successfully processed messages"
        }
    ],
    "templating": {
        "list": [
            {
                "current": {
                    "selected": false,
                    "text": "All",
                    "value": "$__all"
                },
                "datasource": {
                    "type": "prometheus",
                    "uid": "__PROMETHEUS_DATA_SOURCE__"
                },
                "definition": "label_values(kube_pod_status_phase{phase=\"Running\", pod=~\".*consumer.*\"}, pod)",
                "hide": 0,
                "includeAll": true,
                "name": "pod",
                "query": "label_values(kube_pod_status_phase{phase=\"Running\", pod=~\".*consumer.*\"}, pod)",
                "refresh": 2,
                "regex": "",
                "skipUrlSync": false,
                "sort": 1,
                "type": "query"
            },
            {
                "current": {
                    "selected": false,
                    "text": "All",
                    "value": "$__all"
                },
                "datasource": {
                    "type": "prometheus",
                    "uid": "__PROMETHEUS_DATA_SOURCE__"
                },
                "definition": "label_values(consumer_metrics_messages_processed_total, subject)",
                "hide": 0,
                "includeAll": true,
                "name": "subject",
                "query": "label_values(consumer_metrics_messages_processed_total, subject)",
                "refresh": 2,
                "regex": "",
                "skipUrlSync": false,
                "sort": 1,
                "type": "query"
            }
        ]
    },
    "refresh": "10s",
    "timepicker": {
        "refresh_intervals": [
            "5s",
            "10s",
            "30s",
            "1m",
            "5m",
            "15m",
            "30m",
            "1h",
