{
    "annotations": {
        "list": [
            {
                "builtIn": 1,
                "datasource": {
                    "type": "grafana",
                    "uid": "-- Grafana --"
                },
                "enable": true,
                "hide": true,
                "iconColor": "rgba(0, 211, 255, 1)",
                "name": "Annotations & Alerts",
                "type": "dashboard"
            },
            {
                "datasource": {
                    "type": "prometheus",
                    "uid": "__PROMETHEUS_DATA_SOURCE__"
                },
                "enable": true,
                "expr": "sum(changes(kube_deployment_status_observed_generation{deployment=\"webserver\"}[5m])) by (deployment)",
                "iconColor": "#5794F2",
                "name": "Deployments",
                "tagKeys": [
                    "deployment"
                ],
                "titleFormat": "Webserver Deployment"
            }
        ]
    },
    "links": [
        {
            "asDropdown": true,
            "icon": "external-link-alt",
            "includeVars": true,
            "keepTime": true,
            "tags": [
                "fuel-streams"
            ],
            "targetBlank": false,
            "title": "Related Dashboards",
            "type": "dashboards"
        }
    ],
    "panels": [
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 0
            },
            "title": "Resource Usage",
            "type": "row"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "CPU Cores",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 80
                            },
                            {
                                "color": "red",
                                "value": 90
                            }
                        ]
                    },
                    "unit": "cores"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 1
            },
            "targets": [
                {
                    "expr": "sum by (pod) (rate(container_cpu_usage_seconds_total{container=\"webserver\", pod=~\"$pod\"}[5m]))",
                    "legendFormat": "{{pod}}"
                }
            ],
            "title": "CPU Usage by Pod [5m]",
            "description": "CPU consumption in cores per pod",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Memory",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "unit": "bytes"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 1
            },
            "targets": [
                {
                    "expr": "avg_over_time(container_memory_working_set_bytes{container=\"webserver\", pod=~\"$pod\"}[5m])",
                    "legendFormat": "{{pod}} - usage"
                },
                {
                    "expr": "avg_over_time(kube_pod_container_resource_limits{container=\"webserver\", pod=~\"$pod\", resource=\"memory\"}[5m])",
                    "legendFormat": "{{pod}} - limit"
                }
            ],
            "title": "Memory Usage by Pod",
            "description": "Memory usage and limits per pod",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Network I/O",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "unit": "Bps"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 9
            },
            "targets": [
                {
                    "expr": "sum by (pod) (rate(container_network_receive_bytes_total{pod=~\"$pod\"}[5m]))",
                    "legendFormat": "{{pod}} - receive"
                },
                {
                    "expr": "sum by (pod) (rate(container_network_transmit_bytes_total{pod=~\"$pod\"}[5m]))",
                    "legendFormat": "{{pod}} - transmit"
                }
            ],
            "title": "Network I/O by Pod [5m]",
            "description": "Network traffic in/out per pod",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Percentage",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        },
                        "max": 100,
                        "min": 0
                    },
                    "unit": "percent"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 9
            },
            "targets": [
                {
                    "expr": "sum by (pod) (rate(container_cpu_usage_seconds_total{container=\"webserver\", pod=~\"$pod\"}[5m])) / sum by (pod) (kube_pod_container_resource_limits{container=\"webserver\", pod=~\"$pod\", resource=\"cpu\"}) * 100",
                    "legendFormat": "{{pod}} - CPU %"
                },
                {
                    "expr": "avg_over_time(container_memory_working_set_bytes{container=\"webserver\", pod=~\"$pod\"}[5m]) / avg_over_time(kube_pod_container_resource_limits{container=\"webserver\", pod=~\"$pod\", resource=\"memory\"}[5m]) * 100",
                    "legendFormat": "{{pod}} - Memory %"
                }
            ],
            "title": "Resource Usage Percentage",
            "description": "CPU and Memory usage as percentage of limits",
            "type": "timeseries"
        },
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 18
            },
            "title": "Connection Metrics",
            "type": "row"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Duration (seconds)",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "log"
                        }
                    },
                    "unit": "s"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 19
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum by (le, user_name) (rate(ws_connection_duration_seconds_bucket{user_name=~\"$user\"}[5m])))",
                    "legendFormat": "p95 - {{user_name}}"
                },
                {
                    "expr": "histogram_quantile(0.50, sum by (le, user_name) (rate(ws_connection_duration_seconds_bucket{user_name=~\"$user\"}[5m])))",
                    "legendFormat": "p50 - {{user_name}}"
                }
            ],
            "title": "WebSocket Connection Duration by User [5m]",
            "description": "Distribution of how long WebSocket connections stay alive",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Connections",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "unit": "short"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 19
            },
            "targets": [
                {
                    "expr": "sum by (user_name) (ws_active_connections{user_name=~\"$user\"})",
                    "legendFormat": "{{user_name}}"
                }
            ],
            "title": "Active Connections by User",
            "description": "Number of current WebSocket connections per user",
            "type": "timeseries"
        },
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 27
            },
            "title": "Subscription Metrics",
            "type": "row"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Subscriptions",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "unit": "short"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 28
            },
            "targets": [
                {
                    "expr": "sum by (user_name) (ws_user_active_subscriptions{user_name=~\"$user\"})",
                    "legendFormat": "{{user_name}}"
                }
            ],
            "title": "Active Subscriptions by User",
            "description": "Number of current active subscriptions per user",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Duration (seconds)",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "unit": "s"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 28
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum by (le, user_name) (rate(ws_subscription_lifetime_seconds_bucket{user_name=~\"$user\"}[5m])))",
                    "legendFormat": "p95 - {{user_name}}"
                }
            ],
            "title": "Subscription Lifetime by User [5m]",
            "description": "How long subscriptions typically stay active",
            "type": "timeseries"
        },
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 36
            },
            "title": "Error and Warning Metrics",
            "type": "row"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Attempts/sec",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "red",
                                "value": 1
                            }
                        ]
                    },
                    "unit": "ops"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 37
            },
            "targets": [
                {
                    "expr": "sum by (user_name, subscription_id) (rate(ws_duplicate_subscription_attempts{user_name=~\"$user\"}[5m]))",
                    "legendFormat": "{{user_name}} - {{subscription_id}}"
                }
            ],
            "title": "Duplicate Subscription Attempts [5m]",
            "description": "Rate of attempts to create already existing subscriptions",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "__PROMETHEUS_DATA_SOURCE__"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Errors/sec",
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "fillOpacity": 20,
                        "spanNulls": false,
                        "scaleDistribution": {
                            "type": "linear"
                        }
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "red",
                                "value": 1
                            }
                        ]
                    },
                    "unit": "errors/s"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 37
            },
            "targets": [
                {
                    "expr": "sum by (subject, error_type) (rate(ws_streamer_metrics_subs_messages_error_rates{subject=~\"$subject\"}[5m]))",
                    "legendFormat": "{{subject}} - {{error_type}}"
                }
            ],
            "title": "Subscription Message Errors [5m]",
            "description": "Rate of errors in subscription message processing",
            "type": "timeseries"
        }
    ],
